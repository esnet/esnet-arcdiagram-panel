{
  "extends": "./.config/tsconfig.json",
  "include": [
    "src",
    "types"
  ],
  "compilerOptions": {
    "jsx": "react",
    "rootDir": "./src",
    "baseUrl": "./src",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "noImplicitAny": false,
    "allowJs": true,
    "outDir": "$$ts-jest$$",
    "moduleResolution": "node",
    "noEmit": true,
    "module": "ES6",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "public",
    "dist",
    "node_modules"
  ]
}